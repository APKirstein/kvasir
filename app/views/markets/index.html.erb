<div class="level3">
	<h3>Update at: <p id="time"></p></h3>
	<table>
		<tr>
			<th>Station</th>
			<th>Item</th>
			<th>Price</th>
			<% if current_user %>
				<th>Kvasir Index</th>
			<% end %>
		</tr>
		<% @items.each do |item, value| %>
			<tr>
				<td id="regionid">Amarr</td>
				<td id="name"><%= item.name %></td>
				<td id="price"><%= item.price %></td>
				<td id="price"><%= item.price %></td>
				<% if current_user %>
					<td id="kvasir">(kvasir index)</td>
				<% end %>
			</tr>
		<% end %>
	</table>
</div>
<div class="row">
	<p><%= paginate @items %></p>
</div>
<ul id="header">
	<li><%= link_to "Home", root_path %></li>
</ul>
<div class="level4">
	<script src="nccsScripts.js"></script>
</div>
